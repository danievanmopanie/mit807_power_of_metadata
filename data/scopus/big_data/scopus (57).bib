@CONFERENCE{Chikharev2021,
author={Chikharev, I.A. and Tikunova, I.N. and Fedorova, E.A.},
title={Mediterranean city: Constructing a social attractor (the example of Barcelona)},
journal={IOP Conference Series: Earth and Environmental Science},
year={2021},
volume={740},
number={1},
doi={10.1088/1755-1315/740/1/012009},
art_number={012009},
note={cited By 0; Conference of 2020 Scientific and Practical Conference on Cities as Development Centers, CDCcon 2020 ; Conference Date: 21 August 2020 Through 22 August 2020;  Conference Code:168724},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85105476886&doi=10.1088%2f1755-1315%2f740%2f1%2f012009&partnerID=40&md5=4f8bdd4d595fed797c57dca11697e2fe},
affiliation={Department of Management of Science-Intensive Sectoral and Regional Projects, National Research Nuclear University, MEPhI, Moscow, Russian Federation; Faculty of Geography, Moscow State University Named after M.V. Lomonosov, Moscow, Russian Federation},
abstract={The paper analyzes the experience of Barcelona in constructing a city as a social attractor, an area to which the global socio-economic system gravitates. The role of positive changes in the urban environment, the introduction of intelligent technologies of urban management, linking the city with global flows in ensuring the attractiveness of the Mediterranean city are consistently analyzed. The authors define the fundamental structure of the Mediterranean urban attractor, including openness and accessibility of the sea, internal and external transport accessibility, comfortable urban environment, introduction of smart city technologies and connectivity with the global economy. © Published under licence by IOP Publishing Ltd.},
author_keywords={appeal of the city;  attractor;  Barcelona;  Mediterranean;  smart city;  urban environment},
keywords={Economics;  Urban planning, Fundamental structures;  Global economies;  Intelligent technology;  Mediterranean cities;  Positive changes;  Socio-economic systems;  Urban environments;  Urban management, Urban transportation},
funding_details={European Regional Development FundEuropean Regional Development Fund, ERDF},
funding_text 1={In 2011, Barcelona adopted a new urban strategy "Barcelona, City of People", aimed at introducing innovative technologies into the urban environment to improve the efficiency of urban management, stimulate economic growth and improve the well-being of citizens. The main goal of the adopted strategy was to consolidate the city's resources and direct them to increase the efficiency of the urban environment. The strategy is divided into 22 programs covering all areas of urban management. The main role in achieving this goal was played by the creation of a telecommunication network and the city platform CityOS for collecting and accumulating relevant information (air quality, temperature, noise level, road traffic, energy efficiency, etc.), which can be used jointly in management issues, and also for predicting and preventing emergencies. The City OS was developed with financial support from the European Regional Development Fund for Catalonia under the Cataluña ERDF 2014-20 OP program aimed at accelerating economic growth in Catalonia and Europe, as well as developing a green economy. The Barcelona City Council has partnered with US-based technology company CISCO and other technology partners to create open source Sentilo software using data from the Spanish Libelium's Waspmote sensors. Waspmotes sensors collect information and transmit it to the Sentilo platform via ZigBee and 802.15.4 at 868 MHz and 900 MHz, as well as Wi-Fi, BLE and 3G / GPRS. It's worth noting that since Sentilo software is open source, it can be adopted by any city or business.},
